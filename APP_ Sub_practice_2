package com.example.test;

import androidx.appcompat.app.AppCompatActivity;

import android.content.Intent;
import android.os.Bundle;
import android.widget.TextView;

public class Loan_interest extends AppCompatActivity {

    private TextView text_sub;

    private TextView text_sub2;

    private TextView text_sub3;

    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_loan_interest);
        text_sub = findViewById(R.id.text_sub);
        text_sub2 = findViewById(R.id.text_sub2);
        text_sub3 = findViewById(R.id.text_sub3);

        Intent intent2 = getIntent();
        String str = intent2.getStringExtra("str");

        Intent intent3 = getIntent();
        String btr = intent3.getStringExtra("btr");

        Intent intent4 = getIntent();
        String ctr = intent4.getStringExtra("ctr");

        text_sub.setText(str);
        text_sub2.setText(btr);
        text_sub3.setText(ctr);

    }

}
