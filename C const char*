#include <stdio.h>
//xcode-select --install

int main(){
int low, high;
int Temp=0;
int array[6];


scanf("%d %d", &low, &high);


for (int i=0; i<6; i++) {
scanf("%d",&array[i]);
}

for (int j=0; j<low; j++) {
    for (int i=0; i<high; i++) {
        array[Temp]=pow(array[Temp],2);
        printf("%d ",array[Temp]);
        Temp=Temp+1;
    }
    printf("\n");
    
}

return 0;
}


